                    </StackPanel>
                </Fluent:RibbonGroupBox>
            </Fluent:RibbonTabItem>
            <Fluent:RibbonTabItem x:Name="tabrep" Header="{DynamicResource m_preview}" Visibility="Visible" Group="{Binding ElementName=repGroup}">
                <Fluent:RibbonGroupBox Header="{DynamicResource m_repcol}">
                    <StackPanel Orientation="Horizontal">
