                            <Fluent:TextBox x:Name="repdatecontrtextBox" TextChanged="repdatecontrtextBox_TextChanged"  IsEnabled="False" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="40" Text="" PreviewTextInput="qcadnumt4extBox_PreviewTextInput" MaxLength="3"/>
