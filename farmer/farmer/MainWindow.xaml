                                <Fluent:CheckBox x:Name="mapcadnumcheckBox" Header="{DynamicResource m_cadnum}" Checked="mapcadnumcheckBox_Checked" Unchecked="mapcadnumcheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="mapleasercomboBox" IsEnabled="False" SelectionChanged="mapleasercomboBox_SelectionChanged"  VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:ComboBox x:Name="mapstatuscomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="mapstatuscomboBox_SelectionChanged" DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:TextBox x:Name="mapcadnumtextBox" IsEnabled="False" VerticalContentAlignment="Center" TextChanged="mapcadnumtextBox_TextChanged" HorizontalAlignment="Left" Width="145" Text="" MaxLength="22"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5" >
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="mapformownercheckBox" Header="{DynamicResource m_form_owner}" Checked="mapformownercheckBox_Checked" Unchecked="mapformownercheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapformunitcheckBox" Header="{DynamicResource m_form_unit}" Checked="mapformunitcheckBox_Checked" Unchecked="mapformunitcheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapformusecheckBox" Header="{DynamicResource m_form_use}" Checked="mapformusecheckBox_Checked" Unchecked="mapformusecheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="mapformownercomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="mapformownercomboBox_SelectionChanged"  DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:ComboBox x:Name="mapformunitcomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="mapformunitcomboBox_SelectionChanged"  DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:ComboBox x:Name="mapformusecomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="mapformusecomboBox_SelectionChanged"  DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5" >
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="maptypecheckBox" Header="{DynamicResource m_type_contract}" Checked="maptypecheckBox_Checked" Unchecked="maptypecheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapareacheckBox" Header="{DynamicResource m_area_number}" Checked="mapareacheckBox_Checked" Unchecked="mapareacheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapunitcheckBox" Header="{DynamicResource m_unit_number}" Checked="mapunitcheckBox_Checked"  Unchecked="mapunitcheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="maptypecomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="maptypecomboBox_SelectionChanged" DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:TextBox x:Name="mapareatextBox" Text="" IsEnabled="False" TextChanged="mapareatextBox_TextChanged" Width="145"/>
                                <Fluent:TextBox x:Name="mapunittextBox" Text="" IsEnabled="False" TextChanged="mapunittextBox_TextChanged" Width="145"/>
                            </StackPanel>
                        </StackPanel>
                        </StackPanel>
                        <Fluent:ToggleButton x:Name="mapmoreparam" Header="{DynamicResource m_moreopt}" Click="Button_Click_1"  LargeIcon="\Images\search.png"/>
                    </Fluent:RibbonGroupBox>                    
                    <Fluent:RibbonGroupBox>
                        <Fluent:Button x:Name="addkmlbutton" Header="KML"  Click="addkmlbutton_Click" LargeIcon="\Images\add.png"/>
                    </Fluent:RibbonGroupBox>
                </Fluent:RibbonTabItem>
            <Fluent:RibbonTabItem x:Name="contrtabfil" Header="{DynamicResource m_contrparam}" Visibility="Visible" Group="{Binding ElementName=filGroup}">

                <Fluent:RibbonGroupBox Header="{DynamicResource m_extraopt}">

                    <StackPanel Orientation="Horizontal">
                        <StackPanel >
                            <TextBlock HorizontalAlignment="Left" Text="{DynamicResource m_date_contract}" />
                            <StackPanel Orientation="Horizontal">
                                <StackPanel >
                                    <StackPanel Height="2.5"/>
                                    <Fluent:CheckBox x:Name="datacontscheckBox" Header="{DynamicResource m_s}" Checked="datacontscheckBox_Checked" Unchecked="datacontscheckBox_Checked"/>
                                    <StackPanel Height="2.5"/>
                                    <Fluent:CheckBox x:Name="datacontpocheckBox" Header="{DynamicResource m_po}" Checked="datacontpocheckBox_Checked" Unchecked="datacontpocheckBox_Checked"/>
                                </StackPanel>
                                <StackPanel >
                                    <DatePicker x:Name="datacontrsdatePicker" IsEnabled="False" SelectedDateChanged="datacontrsdatePicker_SelectedDateChanged" VerticalContentAlignment="Center" SelectedDate="{x:Static sys:DateTime.Now}" HorizontalAlignment="Left" Width="145" />
                                    <DatePicker x:Name="datacontrpodatePicker" IsEnabled="False" SelectedDateChanged="datacontrpodatePicker_SelectedDateChanged"  VerticalContentAlignment="Center" SelectedDate="{x:Static sys:DateTime.Now}" HorizontalAlignment="Left" Width="145" />
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5" >
                        </StackPanel>
                        <StackPanel >
                            <TextBlock HorizontalAlignment="Left" Text="{DynamicResource m_registration_date}" />
                            <StackPanel Orientation="Horizontal">
                                <StackPanel >
                                    <StackPanel Height="2.5"/>
                                    <Fluent:CheckBox x:Name="dataregscheckBox" Header="{DynamicResource m_s}" Checked="dataregscheckBox_Checked" Unchecked="dataregscheckBox_Checked"/>
                                    <StackPanel Height="2.5"/>
                                    <Fluent:CheckBox x:Name="dataregpocheckBox" Header="{DynamicResource m_po}" Checked="dataregpocheckBox_Checked" Unchecked="dataregpocheckBox_Checked"/>
                                </StackPanel>
                                <StackPanel >
                                    <DatePicker x:Name="dataregsdatePicker" IsEnabled="False" SelectedDateChanged="dataregsdatePicker_SelectedDateChanged"  VerticalContentAlignment="Center" SelectedDate="{x:Static sys:DateTime.Now}" HorizontalAlignment="Left" Width="145" />
                                    <DatePicker x:Name="dataregpodatePicker" IsEnabled="False" SelectedDateChanged="dataregpodatePicker_SelectedDateChanged"  VerticalContentAlignment="Center" SelectedDate="{x:Static sys:DateTime.Now}" HorizontalAlignment="Left" Width="145" />
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5" >
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="oblcheckBox" Header="{DynamicResource m_obl}" Checked="oblcheckBox_Checked" Unchecked="oblcheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="raycheckBox" Header="{DynamicResource m_ray}" Checked="raycheckBox_Checked" Unchecked="raycheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="silcheckBox" Header="{DynamicResource m_sil}" Checked="silcheckBox_Checked" Unchecked="silcheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="oblcomboBox" IsEnabled="False" Width="145" SelectionChanged="oblcomboBox_SelectionChanged"/>
                                <Fluent:ComboBox x:Name="raycomboBox" IsEnabled="False" SelectionChanged="raycomboBox_SelectionChanged" Width="145"/>
                                <Fluent:ComboBox x:Name="silcomboBox" IsEnabled="False" SelectionChanged="silcomboBox_SelectionChanged" Width="145"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5" >
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="typecheckBox" Header="{DynamicResource m_type_contract}" Checked="typecheckBox_Checked" Unchecked="typecheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="areacheckBox" Header="{DynamicResource m_area_number}" Checked="areacheckBox_Checked" Unchecked="areacheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="unitcheckBox" Header="{DynamicResource m_unit_number}" Checked="unitcheckBox_Checked"  Unchecked="unitcheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="typecomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="typecomboBox_SelectionChanged" DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:TextBox x:Name="areatextBox" Text="" IsEnabled="False" TextChanged="areatextBox_TextChanged" Width="145"/>
                                <Fluent:TextBox x:Name="unittextBox" Text="" IsEnabled="False" TextChanged="unittextBox_TextChanged" Width="145"/>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </Fluent:RibbonGroupBox>
            </Fluent:RibbonTabItem>
