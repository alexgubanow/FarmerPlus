                                    <DatePicker x:Name="mapdatacontrpodatePicker" IsEnabled="False" SelectedDateChanged="mapdatacontrpodatePicker_SelectedDateChanged"  VerticalContentAlignment="Center" SelectedDate="{x:Static sys:DateTime.Now}" HorizontalAlignment="Left" Width="145" />
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5" >
                        </StackPanel>
                        <StackPanel >
                            <TextBlock HorizontalAlignment="Left" Text="{DynamicResource m_registration_date}" />
