                    <StackPanel Orientation="Horizontal">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="mapleasercheckBox" Header="{DynamicResource m_leaser}" Checked="mapleasercheckBox_Checked"  Unchecked="mapleasercheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapstatuscheckBox" Header="{DynamicResource m_status}" Checked="mapstatuscheckBox_Checked" Unchecked="mapstatuscheckBox_Checked"/>
