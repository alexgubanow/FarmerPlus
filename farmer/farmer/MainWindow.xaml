                                <Fluent:CheckBox x:Name="mapoblcheckBox" Header="{DynamicResource m_obl}" Checked="mapoblcheckBox_Checked" Unchecked="mapoblcheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapraycheckBox" Header="{DynamicResource m_ray}" Checked="mapraycheckBox_Checked" Unchecked="mapraycheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="mapsilcheckBox" Header="{DynamicResource m_sil}" Checked="mapsilcheckBox_Checked" Unchecked="mapsilcheckBox_Checked"/>
