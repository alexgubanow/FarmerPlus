<Fluent:RibbonWindow xmlns:awe="http://schemas.awesomium.com/winfx" 
    x:Name="webWindow"
    x:Class="farmer.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Fluent="clr-namespace:Fluent;assembly=Fluent"
    xmlns:core="clr-namespace:Awesomium.Core;assembly=Awesomium.Core"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:farmer"
    xmlns:md="clr-namespace:MigraDoc.Rendering.Windows;assembly=MigraDoc.Rendering-WPF"
    xmlns:res="clr-namespace:farmer.Properties"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:mahh="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
    mc:Ignorable="d"
    FlowDirection="LeftToRight"
    Title="Farmer+"
    Height="768"
    Width="1024"
    Loaded="RibbonWindow_Loaded"
    Closing="RibbonWindow_Closing"
    WindowState="Maximized"
    WindowStartupLocation="CenterScreen" Icon="icon.ico" >
    <awe:WebDialogsLayer>
    <Grid>
        <mahh:ProgressRing Panel.ZIndex="1001" x:Name="overlayprogressring" Visibility="Collapsed" Foreground="White"/>
        <Rectangle Panel.ZIndex="1000" x:Name="overlayrect" Fill="DarkGray" Opacity=".5" Visibility="Collapsed"/>
        <Fluent:Ribbon SelectedTabIndex="2" AutomaticStateManagement="True" x:Name="ribbon" SelectedTabChanged="Ribbon_SelectedTabChanged" CanMinimize="False" CanQuickAccessLocationChanging="False" CanCustomizeQuickAccessToolBarItems="False">
            <Fluent:Ribbon.Menu>
                <Fluent:Backstage x:Name="backMenu" Header="{DynamicResource m_menu}">
                    <Fluent:BackstageTabControl>
                        <Fluent:BackstageTabItem Header="{DynamicResource m_settings}">
                                <StackPanel >
                                    <GroupBox Header="{DynamicResource m_autorize}">
                                        <StackPanel Orientation="Horizontal">
                                            <Fluent:CheckBox x:Name="enablepasscheckBox" Header="{DynamicResource m_enautorize}" Checked="enablepasscheckBox_Checked" Unchecked="enablepasscheckBox_Checked"/>
                                            <StackPanel Width="10"/>
                                            <Button Content="{DynamicResource m_changepass}" Click="Button_Click_3"/>
                                        </StackPanel>
                                    </GroupBox>
                                    <GroupBox Header="{DynamicResource m_editdb}">
                                        <StackPanel Orientation="Horizontal">
                                            <Fluent:CheckBox x:Name="enableeditcheckBox" Header="{DynamicResource m_enableeditdb}" Checked="enableeditcheckBox_Unchecked" Unchecked="enableeditcheckBox_Unchecked"/>
                                        </StackPanel>
                                    </GroupBox>
                                    <GroupBox Header="{DynamicResource m_connservdb}">
                                    <StackPanel>
                                        <StackPanel Height="3"/>
                                        <StackPanel Orientation="Horizontal">
                                            <StackPanel Width="3"/>
                                            <StackPanel >
                                                <StackPanel Height="3"/>
                                                <TextBlock Text="{DynamicResource m_servip}" />
                                                <StackPanel Height="8"/>
                                                <TextBlock Text="{DynamicResource m_user}" />
                                                <StackPanel Height="8"/>
                                                <TextBlock Text="{DynamicResource m_pass}" />
                                            </StackPanel>
                                            <StackPanel >
                                                <Fluent:TextBox x:Name="serveriptextBox" HorizontalAlignment="Left" Width="145" Text=""/>
                                                <StackPanel Height="3"/>
                                                <Fluent:TextBox x:Name="usertextBox" HorizontalAlignment="Left" Width="145" Text="" />
                                                <StackPanel Height="3"/>
                                                <Fluent:TextBox x:Name="passtextBox" HorizontalAlignment="Left" Width="145" Text=""/>
                                            </StackPanel>
                                            <StackPanel Width="8"/>
                                            <StackPanel >
                                                <StackPanel Orientation="Horizontal">
                                                    <StackPanel >
                                                        <StackPanel Height="3"/>
                                                        <TextBlock Text="{DynamicResource m_servport}" />
                                                        <StackPanel Height="8"/>
                                                        <TextBlock Text="{DynamicResource m_db}" />
                                                    </StackPanel>
                                                    <StackPanel >
                                                        <Fluent:TextBox x:Name="serverporttextBox" HorizontalAlignment="Left" Width="145" Text="" />
                                                        <StackPanel Height="3"/>
                                                        <Fluent:TextBox x:Name="dbtextBox" HorizontalAlignment="Left" Width="145" Text="" />
                                                    </StackPanel>
                                                </StackPanel>
                                                <StackPanel Height="4"/>
                                                <Button x:Name="testbutton" Content="{DynamicResource m_testsave}" Click="testbutton_Click"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Header="{DynamicResource m_db}">
                                    <StackPanel>
                                        <StackPanel Height="3"/>
                                        <StackPanel Orientation="Horizontal">
                                            <StackPanel Width="3"/>
                                            <StackPanel >
                                                <Button x:Name="cleardbbutton" Content="{DynamicResource m_delallcontrs}" Click="cleardbbutton_Click"/>
                                                <StackPanel Height="5"/>
                                                <Button x:Name="backupdbbutton" Content="{DynamicResource m_backupdb}" Click="backupdbbutton_Click"/>
                                                <StackPanel Height="4"/>
                                                <Button x:Name="restoredbbutton" Content="{DynamicResource m_restorebackupdb}" Click="restoredbbutton_Click"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                </GroupBox>
                                <GroupBox Header="{DynamicResource m_language}">
                                    <StackPanel>
                                        <StackPanel Height="3"/>
                                        <StackPanel Orientation="Horizontal">
                                            <StackPanel Width="3"/>
                                            <ComboBox x:Name="langcomboBox" SelectedValuePath="Key" DisplayMemberPath="Value" Height="26" Width="150" SelectionChanged="langcomboBox_SelectionChanged"/>
                                        </StackPanel>
                                    </StackPanel>
                                </GroupBox>
                            </StackPanel>
                        </Fluent:BackstageTabItem>
                        <!--<Fluent:BackstageTabItem Header="О программе"/>-->
                        <Fluent:Button Header="{DynamicResource m_closeprog}" Click="Button_Click" />
                    </Fluent:BackstageTabControl>
                </Fluent:Backstage>
            </Fluent:Ribbon.Menu>
            <Fluent:Ribbon.ContextualGroups>
                <Fluent:RibbonContextualTabGroup Visibility="Visible" x:Name="filGroup" Header="{DynamicResource m_fil}" Background="Purple" BorderBrush="Purple" />
                <Fluent:RibbonContextualTabGroup Visibility="Visible" x:Name="repGroup" Header="{DynamicResource m_print}" Background="Green" BorderBrush="Green" />
            </Fluent:Ribbon.ContextualGroups>
            <Fluent:RibbonTabItem x:Name="tabcontr" Header="{DynamicResource m_contracts}">
                <Fluent:RibbonGroupBox Header="{DynamicResource m_report}">
                    <Fluent:Button LargeIcon="\Images\PasteLarge.png" Header="{DynamicResource m_genrep}" Click="filteredMenuItem_Click" />
                </Fluent:RibbonGroupBox>
                <Fluent:RibbonGroupBox Header="{DynamicResource m_import}">
                    <Fluent:Button Header="{DynamicResource m_manualinput}" Click="addcontbutton_Click" LargeIcon="\Images\add.png"/>
                    <Fluent:Button Header="XML" Click="addcontXMLbutton_Click" LargeIcon="\Images\xmlLarge.png" Width="57"/>
                </Fluent:RibbonGroupBox>
                <Fluent:RibbonGroupBox x:Name="tabcontrfilgroup" Header="{DynamicResource m_fil}">
                    <StackPanel Orientation="Horizontal">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="leasercheckBox" Header="{DynamicResource m_leaser}" Checked="leasercheckBox_Checked"  Unchecked="leasercheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="statuscheckBox" Header="{DynamicResource m_status}" Checked="statuscheckBox_Checked" Unchecked="statuscheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="cadnumcheckBox" Header="{DynamicResource m_cadnum}" Checked="cadnumcheckBox_Checked" Unchecked="cadnumcheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="leasercomboBox" IsEnabled="False" SelectionChanged="leasercomboBox_SelectionChanged"  VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:ComboBox x:Name="statuscomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="statuscomboBox_SelectionChanged" DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:TextBox x:Name="cadnumtextBox" IsEnabled="False" VerticalContentAlignment="Center" TextChanged="cadnumtextBox_TextChanged" HorizontalAlignment="Left" Width="145" Text="" MaxLength="22"/>
                            </StackPanel>
                        </StackPanel>
                        <StackPanel Width="5"/>
                        <StackPanel Orientation="Horizontal">
                            <StackPanel >
                                <Fluent:CheckBox x:Name="formownercheckBox" Header="{DynamicResource m_form_owner}" Checked="formownercheckBox_Checked" Unchecked="formownercheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="formunitcheckBox" Header="{DynamicResource m_form_unit}" Checked="formunitcheckBox_Checked" Unchecked="formunitcheckBox_Checked"/>
                                <StackPanel Height="2.5"/>
                                <Fluent:CheckBox x:Name="formusecheckBox" Header="{DynamicResource m_form_use}" Checked="formusecheckBox_Checked" Unchecked="formusecheckBox_Checked"/>
                            </StackPanel>
                            <StackPanel >
                                <Fluent:ComboBox x:Name="formownercomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="formownercomboBox_SelectionChanged"  DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:ComboBox x:Name="formunitcomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="formunitcomboBox_SelectionChanged"  DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                                <Fluent:ComboBox x:Name="formusecomboBox" IsEnabled="False" SelectedValuePath="Key" SelectionChanged="formusecomboBox_SelectionChanged"  DisplayMemberPath="Value" VerticalContentAlignment="Center" HorizontalAlignment="Left" Width="145" />
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                    <Fluent:ToggleButton x:Name="contrmoreparam" Header="{DynamicResource m_moreopt}" Click="Button_Click_1"  LargeIcon="\Images\search.png"/>
                </Fluent:RibbonGroupBox>
            </Fluent:RibbonTabItem>
            <Fluent:RibbonTabItem x:Name="tabrotation" Header="Севооборот" Visibility="Collapsed">
            </Fluent:RibbonTabItem>
            <Fluent:RibbonTabItem x:Name="tabmap" Header="{DynamicResource m_map}">
