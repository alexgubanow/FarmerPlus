<Window x:Class="farmer.passwindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:farmer"
        mc:Ignorable="d"
        Title="Farmer+" Height="108.949" Width="176.928" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" Icon="icon.ico" WindowStyle="None" BorderBrush="Black" BorderThickness=".3">
    <Grid>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="90,77,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <PasswordBox x:Name="passwordBox" TabIndex="0" HorizontalAlignment="Left" Height="23" Margin="10,38,0,0" VerticalAlignment="Top" Width="155" KeyDown="passwordBox_KeyDown"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="44,11,0,0" TextWrapping="Wrap" Text="{DynamicResource m_enterpass}" VerticalAlignment="Top"/>
        <Button x:Name="cancelbutton" Content="{DynamicResource m_cancel}" HorizontalAlignment="Left" Margin="10,77,0,0" VerticalAlignment="Top" Width="75" Click="cancelbutton_Click"/>
    </Grid>
</Window>
