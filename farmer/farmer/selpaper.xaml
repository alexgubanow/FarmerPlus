<Window x:Class="farmer.selpaper"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:farmer"
        mc:Ignorable="d"
        Title="selpaper"
        BorderBrush="Black"
        BorderThickness=".5"
        WindowStartupLocation="CenterOwner"
        ResizeMode="NoResize"
        WindowStyle="None"
        ShowInTaskbar="False" Height="91.593" Width="141.371" Loaded="Window_Loaded">
    <Grid>
        <ComboBox x:Name="paperformatcomboBox" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top" Width="120" SelectedValuePath="Key" DisplayMemberPath="Value"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="{DynamicResource m_paperform}" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="31,58,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>
</Window>
